---
import { getAllUsers } from "../../lib/users";
import Layout from "../layouts/../../layouts/Master.astro";
const users = await getAllUsers();
if (!users) {
  return new Response(null, {
    status: 404,
    statusText: "Not found",
  });
}
---

<Layout>
  <h1>Users1</h1>
  <ul>
    {users.map((user) => <li>{user._id}</li>)}
  </ul>
</Layout>
